<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.submissionthree">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SubmissionOne">
        <activity
            android:name="com.example.submissionthree.ui.favorite.FavoriteActivity"
            android:parentActivityName="com.example.submissionthree.ui.home.MainActivity" />
        <activity android:name="com.example.submissionthree.ui.detailtvseries.DetailTvSeriesActivity" />
        <activity
            android:name="com.example.submissionthree.ui.detailmovies.DetailMovieActivity"
            android:label="@string/detailmovie"
            android:parentActivityName="com.example.submissionthree.ui.home.MainActivity" />
        <activity android:name="com.example.submissionthree.ui.home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>